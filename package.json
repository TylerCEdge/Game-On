{
  "name": "gameonapp",
  "version": "1.0.0",
  "description": "Game app using IGDB for managing a wish/watch list for video games",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
